    </test>

    <test preserve-order="true" verbose="2"  allow-return-values="true"  name="操作日志">
        <classes>
            <class name="http.log.action_log"></class>
        </classes>

    </test>

    <test preserve-order="true" verbose="2"  allow-return-values="true"  name="运维日志">
        <classes>
            <class name="http.log.ops_log"></class>
        </classes>

    </test>

    <test preserve-order="true" verbose="2"  allow-return-values="true"  name="首页看板">
        <classes>
            <class name="http.dash_board.node_state"></class>
        </classes>
    </test>

    <test preserve-order="true" verbose="2"  allow-return-values="true"  name="大屏查看">
        <classes>
            <class name="http.dasv.permissions"></class>
        </classes>
    </test>

