<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name=" Test Suite Demo_package">
    <listeners>
        <listener class-name = "com.test.Listener.ExtenTestNGIReporterListenerOld" />
        <listener class-name="com.test.Listener.RePrioritizingListener"></listener>
    </listeners>
    <test preserve-order="true" verbose="2"  allow-return-values="true"  name="初始化用户">
        <classes>
            <class name="init.initUser"></class>
        </classes>
    </test>

    <test preserve-order="true" verbose="2"  allow-return-values="true"  name="安装终端">
        <classes>
        <class name="http.add_assets.get_download_win"></class>
            <class name="http.add_assets.download_process"></class>
        </classes>
    </test>

    <test preserve-order="true" verbose="2"  allow-return-values="true"  name="终端操作">
        <classes>
            <class name="http.asset_overview.assert_overview_process"></class>
        </classes>
    </test>


    <test preserve-order="true" verbose="2"  allow-return-values="true"  name="策略管理流程">
        <classes>
            <class name="http.template.template_porcess"></class>
        </classes>
    </test>

    <test preserve-order="true" verbose="2"  allow-return-values="true"  name="病毒扫描流程">
        <classes>
            <class name="http.virus.virus_process"></class>
        </classes>
    </test>

    <test preserve-order="true" verbose="2"  allow-return-values="true"  name="网马查杀流程">
        <classes>
            <class name="http.network_trojan_killing.network_trojan_process"></class>
