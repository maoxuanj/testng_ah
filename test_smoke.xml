    <test preserve-order="true" verbose="2"  allow-return-values="true"  name="主机发现">
        <classes>
            <class name="http.node_scan.start_scan"></class>
        </classes>
    </test>

    <test preserve-order="true" verbose="2"  allow-return-values="true"  name="漏洞扫描流程">
        <classes>
            <class name="http.vul_manage.vul_manage_process"></class>
        </classes>
    </test>

    <test preserve-order="true" verbose="2"  allow-return-values="true"  name="终端体检">
        <classes>
            <class name="http.node_risk.risk_process"></class>
