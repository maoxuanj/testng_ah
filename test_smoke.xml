    </test>


    <test preserve-order="true" verbose="2"  allow-return-values="true"  name="事件响应">
        <classes>
            <class name="http.event_search.event_search"></class>
        </classes>
    </test>

    <test preserve-order="true" verbose="2"  allow-return-values="true"  name="分组标签">
        <classes>
            <class name="http.groupAndtitle.title"></class>
        </classes>

    </test>


    <test preserve-order="true" verbose="2"  allow-return-values="true"  name="敏感词任务">
        <classes>
            <class name="http.sensitive_words.sensitive_process"></class>
        </classes>

